            module="apps/tumbler"
            revision="master" />
    <dependencies />
  </autotools>

  <autotools id="maemo-video-thumbnailer">
    <branch repo="maemo.gitorious.org"
            module="maemo-video-thumbnailer"
            revision="master" />
    <dependencies />
  </autotools>

  <autotools id="hornsey">
    <branch repo="git.moblin.org"
            module="hornsey"
            revision="master"/>
    <dependencies>
      <dep package="bognor-regis"/>
      <dep package="mx"/>
      <dep package="clutter-gst"/>
      <dep package="clutter-gtk"/>
    </dependencies>
    <suggests>
      <dep package="tracker"/>
      <dep package="tumbler"/>
    </suggests>
  </autotools>

  <autotools id="clutter-gtk">
    <branch repo="git.clutter-project.org"
            module="clutter-gtk"
            revision="clutter-gtk-0.10"/>
    <dependencies>
      <dep package="clutter"/>
    </dependencies>
    <suggests>
      <dep package="gtk+"/>
    </suggests>
  </autotools>

  <autotools id="moblin-icon-theme"
             autogen-sh="create-icon-theme.sh moblin &amp;&amp; cd output/moblin &amp;&amp; ./autogen.sh"
             makeargs="-C output/moblin"
             makeinstallargs="-C output/moblin install">
    <branch repo="git.moblin.org"
            module="moblin-icon-theme"
            revision="master"/>
  </autotools>

  <autotools id="cluttersmith">
    <branch repo="git.clutter-project.org"
            module="cluttersmith"
            revision="master"/>
    <dependencies>
      <dep package="clutter-gst"/>
      <dep package="mx"/>
      <dep package="gjs"/>
      <dep package="moblin-icon-theme"/>
    </dependencies>
  </autotools>

  <!-- deprecated -->

  <autotools id="nbtk">
    <branch repo="git.moblin.org"
            module="nbtk"
            revision="master"/>
    <dependencies>
      <dep package="clutter"/>
      <dep package="clutter-imcontext"/>
      <dep package="libccss"/>
    </dependencies>
  </autotools>

  <autotools id="dalston">
    <branch repo="git.moblin.org"
            module="dalston"
            revision="master"/>
    <dependencies>
    </dependencies>
  </autotools>

  <autotools id="mojito" autogenargs="--with-online=networkmanager">
    <branch repo="git.moblin.org"
            module="mojito"
            revision="master"/>
    <dependencies>
      <dep package="librest"/>
    </dependencies>
  </autotools>

  <autotools id="clutter-mozembed" autogenargs="--enable-plugins">
    <branch repo="git.clutter-project.org"
            module="clutter-mozembed"
            revision="master"/>
    <dependencies>
